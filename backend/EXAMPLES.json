{
  "examples": [
    {
      "title": "Create a Concept with no Prerequisites",
      "method": "POST",
      "endpoint": "/api/concepts",
      "body": {
        "concept_id": "calculus_limits",
        "title": "Understanding Limits",
        "description": "Basic concepts of limits in calculus",
        "category": "Calculus",
        "difficulty_level": 2,
        "prerequisites": []
      }
    },
    {
      "title": "Create a Concept with Prerequisites",
      "method": "POST",
      "endpoint": "/api/concepts",
      "body": {
        "concept_id": "calculus_derivatives",
        "title": "Derivatives",
        "description": "Learn derivatives and differentiation",
        "category": "Calculus",
        "difficulty_level": 3,
        "prerequisites": ["calculus_limits"]
      }
    },
    {
      "title": "Create Complex Prerequisite Chain",
      "method": "POST",
      "endpoint": "/api/concepts",
      "body": {
        "concept_id": "multivariable_calculus",
        "title": "Multivariable Calculus",
        "description": "Calculus with multiple variables",
        "category": "Calculus",
        "difficulty_level": 4,
        "prerequisites": ["calculus_derivatives", "linear_algebra_basics"]
      }
    },
    {
